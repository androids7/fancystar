<?xml version="1.0" encoding="gb2312"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="static_lib_debug"
	>
	<Tool
		Name="VCLibrarianTool"
		OutputFile="$(OutDir)\$(LibPrefix)$(ProjectName)_d.lib"
	/>
	<Tool
		Name="VCPostBuildEventTool"
		CommandLine="copy /Y $(TargetPath) $(LibDir)$(TargetFileName)&#x0D;&#x0A;copy /Y $(TargetDir)$(TargetName).pdb $(LibDir)$(TargetName).pdb"
	/>
</VisualStudioPropertySheet>
